@page
@addTagHelper*, Microsoft.AspNetCore.Mvc.TagHelpers
@model MovieMVCWeb.Pages.SuppliersModel
<div class="row">
<h1 class="display-2">Something Title</h1>
         <table class="table">
           <thead class="thead-inverse">
             <tr><th>Genres for movies</th></tr>
           </thead>
           <tbody>
           @foreach(string name in Model.Suppliers)
           {
             <tr><td>@name</td></tr>
           }
           </tbody>
         </table>
</div>
        

<div class="row">
<p>Enter a name for a new genre:</p>
<form method="POST">
<div><input asp-for="Genre.GenreID"/></div>
<input type="submit" />
</form>
</div>